<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head'); %>
  <nav role="navigation"><%- include('./partials/header'); %></nav>

  <body>
    <h1><%- title %></h1>

    <% data.data.forEach(function(gameData) { %>
    <a href="/games/<%- gameData.id %>">
      <div class="matchButton">
        <h4>
          <%- gameData.home_team.full_name + " VS " +
          gameData.visitor_team.full_name%>
        </h4>
        <p><%- gameData.date %></p>
      </div>
    </a>
    <% }) %>

    <h1></h1>
    <%- include('./partials/loader'); %>
  </body>
</html>
